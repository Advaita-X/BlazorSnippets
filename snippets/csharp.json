{
    "Property" :{
        "prefix": "bprop",
        "description": "Create a property.",
        "body": ["${1:public} ${2:string} ${3:Name} { get; set; }"]
    },
    "parameter" :{
        "prefix": "bpara",
        "description": "Create a Parameter.",
        "body": ["[Parameter]",
         "${1:public} ${2:string} ${3:Name} { get; set; }"]
    },
    "cascadingparameter" :{
        "prefix": "bcpara",
        "description": "Create a Cascading Parameter.",
        "body": ["[CascadingParameter]",
        "${1:public} ${2:string} ${3:Name} { get; set; }"]
    },
    "BlazorGetJsonAsync" :{
        "prefix": "bgja",
        "description": "GetFromJsonAsync",
        "body": ["await ${1:Http}.GetFromJsonAsync<${2:string}>(\"${3:api/}\");"]
    }
}